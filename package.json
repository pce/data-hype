{
  "name": "hype",
  "version": "0.1.0",
  "description": "A minimal, hyperscript-like progressive-enhancement fetch enhancer",
  "keywords": [
    "progressive-enhancement",
    "fetch",
    "html",
    "spa",
    "hypermedia"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/pce/hype.git"
  },
  "type": "module",
  "main": "./dist/hype.cjs",
  "module": "./dist/hype.js",
  "browser": "./dist/hype.iife.js",
  "types": "./dist/hype.d.ts",
  "exports": {
    ".": {
      "import": "./dist/hype.js",
      "require": "./dist/hype.cjs",
      "types": "./dist/hype.d.ts"
    },
    "./min": {
      "import": "./dist/hype.min.js",
      "require": "./dist/hype.min.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "pnpm -v >/dev/null 2>&1 && pnpm run build:core && pnpm run build:rx || npm run build:core && npm run build:rx",
    "build:core": "node scripts/build.js --core",
    "build:rx": "node scripts/build.js --rx",
    "dev": "node scripts/build.js --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "tsc --noEmit",
    "clean": "rm -rf dist coverage docs-output",
    "docs": "sh scripts/docs.sh || typedoc",
    "docs:serve": "typedoc && npx http-server docs-output -p 8080 -o",
    "prepublishOnly": "pnpm -v >/dev/null 2>&1 && pnpm run build || npm run build"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@vitest/coverage-v8": "^1.0.0",
    "esbuild": "^0.19.8",
    "happy-dom": "^12.10.3",
    "typedoc": "^0.25.0",
    "typescript": "^5.3.2",
    "vitest": "^1.0.0",
    "rxjs": "^7.8.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
